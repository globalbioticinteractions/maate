  <methods>
        <methodStep>
          <description>
            <para>Se realiza el recorrido  de 7 am a 11am por cada sendero establecidos dentro del área protegida</para>
          </description>
        </methodStep>
        <methodStep>
          <description>
            <para>Se registran los datos en excel.</para>
          </description>
        </methodStep>
